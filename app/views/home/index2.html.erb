                                            <% if user_signed_in? %>
                                                <% if !@fav.include? p.id %>
                                                <div class="quick-view produk-<%= p.id %>">
                                                  <%= form_for @favorite_produk, remote: true do |f| %>
                                                  <%= f.hidden_field :user_id, value: current_user.id %>
                                                  <%= f.hidden_field :produk_id, value: p.id %>
