                                                <span class="price product-price"><%= number_to_currency(p.harga_diskon, locale: :id) %></span>
                                                <span class="price old-price"><%= number_to_currency(p.harga, locale: :id) %></span>
                                              <% else %>
                                              <span class="price product-price"><%= number_to_currency(p.harga_beli, locale: :id) %></span>
                                              <span style="visibility:hidden;" class="price old-price"></span>
                                              <% end %>
                                            </div>
                                            <div class="product-star">
                                              <% if p.rating.present? %>
                                              <% p.rating.to_i.times do %>
                                                <i class="fa fa-star"></i>
                                              <% end %>
                                              <% (5 - p.rating.to_i).times do %>
                                                <i class="fa fa-star-o"></i>
                                              <% end %>
                                              <% else %>
                                              <i class="fa fa-star-o"></i>
                                              <i class="fa fa-star-o"></i>
                                              <i class="fa fa-star-o"></i>
                                              <i class="fa fa-star-o"></i>
                                              <i class="fa fa-star-o"></i>
                                              <% end %>
                                            </div>
                                        </div>
