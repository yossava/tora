                                                  <% end %>
                                               </div>
                                             <% else %>
                                             <% @myfav = FavoriteProduk.where(:user_id => current_user.id, :produk_id => p.id).first %>
                                            <div class="quick-view produk-<%= p.id %>">
                                            <%= form_tag delfav_path, remote: true do %>
                                            <%= hidden_field_tag :id, @myfav.id %>
                                            <button title="Remove from wishlist" class="heart un" ><i class="fa fa-heart" aria-hidden="true"></i></button>
                                              <a title="Kunjungi Toko" class="compare" href="<%= toko_path(p.toko) %>"></a>
                                            <% end %>
                                            </div>
                                            <% end %>
                                            <% end %>
                                            <div class="add-to-cart">
                                                <a title="Detail Produk" href="<%= produk_path(p) %>">Detail</a>
                                            </div>
                                            <div class="group-price">
                                              <% if (Time.now - p.created_at).to_i/86400 < 3 %>
                                                <span class="product-new">NEW</span>
                                              <% end %>
                                              <% if p.harga_diskon.present? && p.harga_diskon > 0 %>
                                                <span class="product-sale">Sale</span>
                                              <% end %>
                                            </div>
                                        </div>
                                        <div class="right-block">
                                            <h5 class="product-name"><a href="#"><%= p.nama_produk %></a></h5>
                                            <div class="content_price">
